{
  "name": "CSS and XPath checker",
  "version": "0.10.0",
  "description": "A helper tool for to confirm CSS and XPath selector.",
  "icons": {
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "cxchecker.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}